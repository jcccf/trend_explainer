<h1>TwitterCall#index</h1>
<p>Find me in app/views/twitter_call/index.html.erb</p>

<h1>Valid Locations</h1>
<%= @locations %>

<h1>Current Trends</h1>
<%= @trends %>

<% @results.each do |result| %>
  <h3><%= result.trend %></h3>
  <div style="margin-left: 50px">
  <b>Bing:</b> <%= result.bing %><br />
  <b>Altered Search Query:</b> <%= result.altered_query %><br />
  <b>Wikipedia:</b> <%= result.wikipedia %>  
  </div>
<% end%>